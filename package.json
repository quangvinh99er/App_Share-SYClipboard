{
  "name": "SYClipboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "stop-package": "killall -9 node",
    "reset-cache": "npm start --reset-cache",
    "clean": "rm -rf $TMPDIR/react-* && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && rm -rf package-lock.json && rm -rf yarn.lock && npm cache clean -force && yarn",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "bundle-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "apk-release": "cd android && ./gradlew assembleRelease && cd ..",
    "pod-install": "cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install --repo-update && cd ..",
    "pod": "pod install --project-directory=ios",
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "ios": "react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.7.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-firebase/app": "^10.8.0",
    "@react-native-firebase/messaging": "^10.8.0",
    "axios": "^0.21.1",
    "i18next": "^19.8.4",
    "lodash": "^4.17.15",
    "lottie-ios": "3.1.8",
    "lottie-react-native": "^3.5.0",
    "moment": "^2.29.1",
    "npm": "^7.10.0",
    "react": "16.13.1",
    "react-i18next": "^11.8.4",
    "react-native": "0.63.4",
    "react-native-device-info": "^8.1.0",
    "react-native-fast-image": "^5.4.0",
    "react-native-fs": "^2.17.0",
    "react-native-image-base64": "^0.1.4",
    "react-native-image-picker": "^3.3.2",
    "react-native-navigation": "^7.8.4",
    "react-native-photo-editor": "^1.0.10",
    "react-native-toast-message": "^1.4.9",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/runtime": "^7.12.18",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.1",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
